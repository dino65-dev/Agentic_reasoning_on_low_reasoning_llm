{
  "mcpServers": {
    "calculator": {
      "command": "python",
      "args": ["-m", "mcp_server_calculator"],
      "description": "Performs mathematical calculations",
      "enabled": true
    },
    "computing-mcp": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "%TEMP%:/app/shared",
        "ychen94/computing-mcp:latest"
      ],
      "description": "Computing and analysis tools",
      "enabled": true
    },
    "mcpgex": {
      "command": "python3",
      "args": ["-m", "mcpgex"],
      "description": "Graph and network analysis",
      "enabled": true
    }
  },
  "tools": [
    {
      "name": "calculator",
      "description": "Performs mathematical calculations. Input should be a math expression.",
      "server": "calculator",
      "type": "function"
    },
    {
      "name": "compute",
      "description": "Performs complex computations and data analysis.",
      "server": "computing-mcp",
      "type": "function"
    },
    {
      "name": "graph_analyzer",
      "description": "Analyzes graphs and networks.",
      "server": "mcpgex",
      "type": "function"
    }
  ]
}